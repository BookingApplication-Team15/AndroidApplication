        <item name="strokeColor">@color/dark_blue</item>
        <item name="android:fillColor">@color/dark_blue</item>
        <item name="iconTint">@color/dark_blue</item>
<!--        <item name="android:backgroundTint">@color/dark_blue</item>-->
        <!-- Add other attributes if needed -->
